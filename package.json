{
  "name": "StarterKit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:dev": "react-native run-android --mode=devdebug --appId=com.starterkit.dev",
    "android:uat": "react-native run-android --mode=uatrelease --appId=com.starterkit.uat",
    "android:prod": "react-native run-android --mode=prodrelease --appId=com.starterkit",
    "ios:dev": "react-native run-ios --scheme 'StarterKitDev'",
    "ios:uat": "react-native run-ios --scheme 'StarterKitUAT'",
    "ios:prod": "react-native run-ios --scheme 'StarterKit'",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-config": "^1.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.3",
    "@react-native/eslint-config": "0.79.3",
    "@react-native/metro-config": "0.79.3",
    "@react-native/typescript-config": "0.79.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-testing-library": "^7.5.3",
    "jest": "^30.0.2",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
